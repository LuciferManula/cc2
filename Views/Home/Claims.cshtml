@using System.Security.Claims

@{
  ViewData["Title"] = "Claims";
}
<h1>@ViewData["Title"]</h1>

<table class="table">
  <tr>
    <th>Type</th>
    <th>Value</th>
  </tr>

  @foreach (Claim claim in User.Claims)
  {
    <tr>
      <td>@claim.Type</td>
      <td>@claim.Value</td>
    </tr>
  }
</table>